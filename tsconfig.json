        "baseUrl": ".",
        "paths": {
            "@SignInIAShandlers": ["handlers/Sign_Up_IAS_Handler.ts"],
            "@SignInIShandlers": ["handlers/Sign_Up_IS_Handler.ts"],
            "@LogInIAShandlers": ["handlers/Log_In_IAS_Handler.ts"],
